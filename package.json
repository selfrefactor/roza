{
	"name": "roza",
	"version": "0.0.2",
	"description": "Utility library",
	"main": "dist/commonjs/index.js",
	"module": "dist/es/index.js",
	"jsnext:main": "dist/es/index.js",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/selfrefactor/roza.git"
	},
	"author": "Dejan Toteff",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/selfrefactor/roza/issues"
	},
	"homepage": "https://github.com/selfrefactor/roza#readme",
	"devDependencies": {
		"@types/jest": "29.2.3",
    "rambda": "7.4.0",
		"jest": "29.3.1",
		"ts-jest": "29.0.3",
		"ts-node": "10.9.1",
		"dtslint": "4.2.1",
		"typescript": "4.9.3"
	},
	"scripts": {
    "new": "cd scripts && yarn new",
    "==============================SEPARATOR====================================": "",
    "clean": "rm -rf ./dist && mkdir dist",
		"test": "jest --coverage && yarn run compile",
		"test:ci": "jest",
		"test:typings": "dtslint --localTs ./node_modules/typescript/lib --expectOnly ./typings-test",
		"compile": "tsc -p ./tsconfig.json --noEmit",
		"out": "yarn run clean && yarn run build:es && yarn run build:cjs",
		"build:es": "yarn run tsc --declaration true --declarationMap true --module esnext --outDir ./dist/es",
		"build:cjs": "yarn run tsc --declaration true --declarationMap true --module commonjs --outDir ./dist/commonjs"
	},
	"keywords": [
		"utils",
		"fp",
		"functional",
		"programming",
		"typescript"
	],
	"files": [
		"dist/*"
	],
	"jest": {
		"transform": {
			".+\\.ts$": "ts-jest"
		},
		"testRegex": "/src/.*.spec.ts$",
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/dist/"
		],
		"collectCoverageFrom": [
			"src/**/*.ts"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"dependencies": {
		
	}
}
