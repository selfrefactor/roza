{
  "compilerOptions": {
    "target": "es5",
    "module": "ES2015",
    "lib": [
      "es6",
      "es2017.object"
    ],
    "outDir": "./dist",
    "noEmit": false,
    "declaration": false,
    "declarationMap": false,
    "strict": true,
    "strictNullChecks": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
